{
   "expo": {
      "scheme": "willineedit",
      "name": "Will I Need It?",
      "slug": "will-i-need-it",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "description": "a fun little demo app",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
         "**/*"
      ],
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#FFFFFF"
         }
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}
